    "@remix-run/serve": "^1.0.4",
    "@remix-run/vercel": "^1.0.6",
    "@tailwindcss/typography": "^0.5.2",
    "@vercel/build-utils": "^3.1.0",
    "@visx/hierarchy": "^2.1.2",
    "@visx/responsive": "^2.4.1",
    "airtable": "^0.11.1",
    "axios": "^0.24.0",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "gray-matter": "^4.0.3",
    "lru-cache": "^7.10.1",
    "marked": "^4.0.16",
    "mdx-bundler": "^9.0.1",
    "prism-react-renderer": "^1.3.3",
    "qss": "^2.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
