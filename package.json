{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "__prebuild": "rm -rf .vercel",
    "build": "yarn css -m && remix build",
    "__postbuild": "cp -r vercel/* .vercel && cp -r node_modules .vercel/output/functions/index.func && cp -r public .vercel/output/static",
    "css": "tailwindcss -i app/styles/app.css -o app/styles/app.generated.css",
    "dev": "env PORT=3001 concurrently \"yarn css -w\" \"node -r dotenv/config node_modules/.bin/remix dev\"",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@docsearch/css": "^3.1.0",
    "@docsearch/react": "^3.1.0",
    "@octokit/graphql": "^4.8.0",
